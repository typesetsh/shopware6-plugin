    "require": {
        "shopware/core": "*",
        "typesetsh/pdf-bundle": "^9.0.1"
    },
    "extra": {
        "shopware-plugin-class": "Typesetsh\\ShopwarePlatform\\TypesetshShopwarePlatform",
        "copyright": "(c) Jacob Siefer",
        "label": {
            "de-DE": "typeset.sh PDF Generator",
            "en-GB": "typeset.sh PDF Generator"

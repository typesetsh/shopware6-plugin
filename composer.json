{
    "name": "typesetsh/shopware6-plugin",
    "description": "Shopware 6 Typeset.sh PDF Plugin",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacob Siefer",
            "email": "mail@jsiefer.de",
            "role": "Manufacturer"
        },
        {
            "name": "Rouven Alexander Rieker",
            "email": "therouv@googlemail.com",
            "role": "Developer"
        }

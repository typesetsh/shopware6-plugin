<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="Shopware\Core\Checkout\Document\FileGenerator\PdfGenerator" class="Typesetsh\ShopwarePlatform\PdfGenerator">
            <argument>%shopware.filesystem.temp.config.root%</argument>
            <argument>%shopware.filesystem.public.config.root%</argument>

            <tag name="file.generator"/>
        </service>
    </services>
</container>
